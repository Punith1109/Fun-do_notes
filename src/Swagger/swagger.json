  "paths": {
    "/users": {
      "post": {
        "tags": ["Users"],
        "description": "create name...",
        "parameters": [
          {
            "name": "Create name",
            "in": "body",
            "description": "Create",
            "schema": {
              "$ref": "#/definitions/User"
            }
          }
        ],
        "produces": [
          "application/json"
        ],
        "responses": {
          "201": {
            "description": "New user is created"
          },
          "500": {
            "description": "Internal server error"
          }
        }
      }
    },
    "/users/login": {
      "post": {
        "tags": ["Notes"],
        "description": "login using ID...",
        "parameters": [
          {
            "name": "login user",
            "in": "body",
            "description": "login",
            "schema": {
              "$ref": "#/definitions/login"
            }
          }
        ],
        "produces": [
          "application/json"
        ],
        "responses": {
          "200": {
            "description": "Login successful"
          },
          "401": {
            "description": "Unauthorized"
          },
          "500": {
            "description": "Internal server error"
          }
        }
      }
    },
    "/notes": {
      "post": {
        "tags": ["CLA"],
        "description": "Create a new note",
        "parameters": [
          {
            "name": "Create Note",
            "in": "body",
            "description": "Note details",
            "required": true,
            "schema": {
              "$ref": "#/definitions/notes"
            }
          }
        ],
        "responses": {
          "201": {
            "description": "New note is created"
          },
          "500": {
            "description": "Internal server error"
          }
        }
      }
    },
    "/notes/{id}": {
      "post": {
        "tags": ["CLA"],
        "description": "Get note by ID",
        "parameters": [
          {
            "name": "id",
            "in": "path",
            "description": "ID of the note to retrieve",
            "required": true,
            "type": "string"
          }
        ],
        "responses": {
          "200": {
            "description": "Note is retrieved successfully",
            "schema": {
              "$ref": "#/definitions/notes"
            }
          },
          "404": {
            "description": "Note not found"
          },
          "500": {
            "description": "Internal server error"
          }
        }
      }
    },
    "/notes/update/{id}": {
      "put": {
        "tags": ["CLA"],
        "description": "Update note by ID",
        "parameters": [
          {
            "name": "id",
            "in": "path",
            "description": "ID of the note to update",
            "required": true,
            "type": "string"
          },
          {
            "name": "Update note",
            "in": "body",
            "description": "Note details to update",
            "schema": {
              "$ref": "#/definitions/notesUpdate"
            }
          }
        ],
        "responses": {
          "200": {
            "description": "Note is updated successfully"
          },
          "400": {
            "description": "Invalid note supplied"
          },
          "404": {
            "description": "Note not found"
          },
          "500": {
            "description": "Internal server error"
          }
        }
      }
    },
     "notes/delete/{id}": {
      "delete": {
        "tags": ["CLA"],
        "description": "Delete note by ID",
        "parameters": [
          {
            "name": "id",
            "in": "path",
            "description": "ID of the note to delete",
            "required": true,
            "type": "string"
          }
        ],
        "responses": {
          "200": {
            "description": "Note is deleted successfully"
          },
          "404": {
            "description": "Note not found"
          },
          "500": {
            "description": "Internal server error"
          }
        }
      }
    }
  },
  "definitions": {
    "notesUpdate": {
      "type": "object",
      "properties": {
        "title": {
